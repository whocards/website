{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "esModuleInterop": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "~*": ["./src/*"],
      "#*": ["./*"]
    },
    "jsx": "preserve",
    "jsxImportSource": "solid-js"
  }
}
