generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-1.0.x"]
}

datasource db {
  provider = "postgres"
  url      = env("DB_URL")
}

model Purchase {
  id              String   @id
  name            String
  email           String
  slug            String
  price           Int
  netPrice        Int
  date            DateTime
  category        String
  addressProvided Boolean  @default(false)
}
